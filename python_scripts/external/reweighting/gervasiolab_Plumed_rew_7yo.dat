#UNITS ENERGY=kj/mol TIME=fs

h1:       READ FILE=COLVAR_clean  VALUES=h1.lessthan IGNORE_TIME 
cn:       READ FILE=COLVAR_clean  VALUES=cmap_n IGNORE_TIME
#r1:       READ FILE=COLVAR_final  VALUES=r1 IGNORE_TIME
#h2:       READ FILE=COLVAR_final  VALUES=h2.lessthan IGNORE_TIME
m:    READ FILE=COLVAR_clean  VALUES=meta.bias IGNORE_TIME

as: REWEIGHT_METAD TEMP=300 ARG=m.bias

HISTOGRAM ...
  ARG=h1.lessthan,cn
  GRID_MIN=0.0,0.0
  GRID_MAX=5,10
  GRID_BIN=200,200
  BANDWIDTH=0.05,0.05
  LOGWEIGHTS=as
  LABEL=hB
... HISTOGRAM

fes: CONVERT_TO_FES GRID=hB TEMP=300 
DUMPGRID GRID=fes FILE=fes_rew.dat FMT=%8.4f
